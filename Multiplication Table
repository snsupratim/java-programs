import java. util.Scanner;

public class table {
    public static void main(String[] args) {
        System. out.println("Enter a number for seeing multiplication table: ");
        Scanner sc = new Scanner(System.in);
        int number = sc.nextInt();
        for (int i=1; i<=10;i++){
            int result = i*number;
            System.out.format(" %d x %d = %d\n ",i,number,result);
        }
    } 
}
